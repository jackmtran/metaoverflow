extends layout.pug
include utils.pug
block content
  div
    table
      thead
      tbody
      h2(class="h2HomePage")=`${user.username}'s Questions`
      each q in question
        div(id=`question-container-${question.id}`).single-question-container
          a(id=`${q.id}-title` href=`/questions/${q.id}`)= q.title
          p(id=`${q.id}-content`)= q.question
